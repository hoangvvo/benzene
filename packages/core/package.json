{
  "name": "@benzene/core",
  "type": "module",
  "version": "0.5.0",
  "description": "Fast and minimal JavaScript GraphQL Server",
  "author": "Hoang Vo (https://hoangvvo.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoangvvo/benzene.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/hoangvvo/benzene/issues"
  },
  "homepage": "https://benzen.vercel.app/",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./runtimes/jit": {
      "import": "./dist/runtimes/jit.js",
      "require": "./dist/runtimes/jit.cjs"
    },
    "./runtimes/js": {
      "import": "./dist/runtimes/js.js",
      "require": "./dist/runtimes/js.cjs"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "clean": "npx rimraf dist",
    "test": "jest"
  },
  "license": "MIT",
  "dependencies": {
    "@hoangvvo/graphql-jit": "^0.6.2",
    "tiny-lru": "^7.0.6"
  },
  "peerDependencies": {
    "graphql": ">=15"
  },
  "publishConfig": {
    "access": "public"
  }
}
