  "author": "Hoang Vo (https://hoangvvo.com)",
  "keywords": [
    "graphql",
    "util",
    "agnostic",
    "persisted"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoangvvo/benzene.git",
    "directory": "packages/extra"
  },
  "bugs": {
