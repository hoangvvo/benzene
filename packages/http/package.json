{
  "name": "@benzene/http",
  "version": "0.0.0",
  "description": "Fast and minimal GraphQL over HTTP server",
  "author": "Hoang Vo (https://hoangvvo.com)",
  "keywords": [
    "graphql",
    "http",
    "server",
    "performance",
    "agnostic"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoangvvo/benzene.git",
    "directory": "packages/http"
  },
  "bugs": {
    "url": "https://github.com/hoangvvo/benzene/issues"
  },
  "homepage": "https://benzene.vercel.app/",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "esmodule": "dist/index.m.js",
  "exports": {
    ".": {
      "import": "./dist/index.m.js",
      "require": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "microbundle -f modern,cjs --strict --globals @benzene/core=benzeneCore --no-sourcemap --no-compress",
    "clean": "npx rimraf dist",
    "test": "jest"
  },
  "dependencies": {
    "@benzene/core": "^0.3.0"
  },
  "peerDependencies": {
    "graphql": "^14.0.0 || ^15.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
