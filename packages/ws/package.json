{
  "name": "@benzene/ws",
  "version": "0.3.0",
  "description": "GraphQL over WebSocket",
  "author": "Hoang Vo (https://hoangvvo.com)",
  "keywords": [
    "graphql",
    "ws",
    "subscription",
    "websocket"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoangvvo/benzene.git",
    "directory": "packages/ws"
  },
  "bugs": {
    "url": "https://github.com/hoangvvo/benzene/issues"
  },
  "homepage": "https://hoangvvo.github.io/benzene/",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "esmodule": "dist/index.m.js",
